{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "engines": {
    "node": "20.x",
    "npm": ">=9"
  },
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tsc": "tsc --noEmit",
    "test": "vitest run --reporter=dot",
    "test:ui": "vitest run src/pages/settings/SilkPage.test.tsx",
    "test:edge": "vitest --environment edge-runtime --no-browser-env-check",
    "test:db": "vitest run --config vitest.db.config.ts --reporter=json --outputFile=reports/db-tests.json && node -e \"const r=require('./reports/db-tests.json'); if(r.numFailedTests>0){process.exit(1)}\"",
    "test:db:reset": "psql $DATABASE_URL -f tests/sql/_e2e_reset.sql",
    "test:db:seed": "psql $DATABASE_URL -f tests/sql/_e2e_seeds.sql",
    "flyway:migrate": "flyway -locations=filesystem:./database/sql migrate",
    "test:e2e": "vitest run --dir src/e2e --reporter=json --outputFile=reports/e2e-report.json",
    "e2e": "playwright test --reporter=list",
    "lint": "eslint . --max-warnings=200",
    "lint:imports": "madge --circular src && dependency-cruiser src --config dependency-cruiser.config.cjs",
    "lint:fix": "cross-env NODE_PATH=./tools eslint src --fix",
    "codemod:routes": "jscodeshift -t tools/codemods/paths-to-helpers.js 'src/**/*.{ts,tsx,js,jsx}'",
    "build:dev": "vite build --mode development",
    "type-check": "tsc --noEmit",
    "back:audit": "node --loader ts-node/esm scripts/full-back-audit.ts",
    "dev:services": "node --loader ts-node/esm scripts/dev-services.ts",
    "dev:api": "node --loader ts-node/esm services/api/index.ts",
    "test:api": "vitest run --config vitest.api.config.ts",
    "db:migrate": "supabase db push",
    "db:seed": "sh -c 'psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} ${DEMO_SUPABASE_ORG:+-v demo_org_id=\"$DEMO_SUPABASE_ORG\"} -f supabase/seeds/dev_assessments.sql && psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} -f supabase/seeds/dev_emotion_modules.sql && psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} -f supabase/seeds/dev_journal_entries.sql'",
    "db:check": "sh -c 'psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} ${DEMO_SUPABASE_ORG:+-v demo_org_id=\"$DEMO_SUPABASE_ORG\"} -f supabase/tests/assessments_checks.sql && psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} -f supabase/tests/emotion_modules_checks.sql && psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} -f supabase/tests/journal_entries_checks.sql'",
    "db:seed:emotion-modules": "sh -c 'psql \"$DATABASE_URL\" -v ON_ERROR_STOP=1 ${DEMO_SUPABASE_UID:+-v demo_uid=\"$DEMO_SUPABASE_UID\"} -f supabase/seeds/dev_emotion_modules.sql'",
    "db:deploy": "pnpm db:migrate && pnpm db:check",
    "db:refresh:scan": "psql $DATABASE_URL -c 'CALL public.refresh_metrics_scan();'",
    "db:refresh:gam": "psql $DATABASE_URL -c 'CALL public.refresh_metrics_gam();'",
    "db:refresh:vr": "psql $DATABASE_URL -c 'CALL public.refresh_metrics_vr();'",
    "db:refresh:breath": "psql $DATABASE_URL -c 'CALL public.refresh_metrics_breath();'",
    "test:sql": "echo 'Tests SQL désactivés - pgtap-run non disponible'",
    "db:refresh:journal": "psql $DATABASE_URL -c 'CALL public.refresh_metrics_weekly_journal();'",
    "test:db:config": "vitest run --config vitest.db.config.ts",
    "db:test:setup": "node --loader ts-node/esm scripts/test-db-setup.ts",
    "install:npm": "npm install --legacy-peer-deps",
    "clean:install": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "front:ci-install": "npm ci --prefer-offline --audit=false --omit=dev && npm run build",
    "ci:verify-assets": "bash ci/verify-assets.sh",
    "check:env": "node scripts/check-env.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "struct:snapshot": "ts-node scripts/lock-structure.ts",
    "struct:verify": "ts-node scripts/verify-structure.ts",
    "test:quick": "npm run typecheck && npm run lint && npm run test",
    "ci:guard": "npm run lock:check && npm run test:quick && npm run struct:verify",
    "lovable:push": "npm run ci:guard && lovable sync",
    "prepare": "husky",
    "lock:check": "node scripts/validate-lockfile.mjs",
    "generate:ui-registry": "node scripts/generate-ui-registry.mjs",
    "sentry:upload-sourcemaps": "node scripts/upload-sourcemaps.mjs",
    "seed:ci": "tsx scripts/seed-e2e.ts",
    "lhci": "lhci autorun",
    "scan:no-clinical-terms": "node tools/scanners/no-clinical-terms.cjs",
    "scan:no-raw-numbers": "node tools/scanners/no-raw-numbers.cjs",
    "build:analyze": "vite build --mode analyze",
    "build:stats": "npm run build && node scripts/bundle-stats.js",
    "perf:lighthouse": "lhci autorun",
    "perf:sourcemap": "source-map-explorer dist/assets/*.js --html dist/sourcemap.html",
    "pages:distribute": "bash scripts/distribute-pages.sh",
    "pages:validate": "bash scripts/validate-distribution.sh"
  },
