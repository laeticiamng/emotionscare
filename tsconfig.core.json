{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noImplicitOverride": false,
    "target": "ES2020",
    "types": [
      "vite/client"
    ],
    "useDefineForClassFields": true,
    "noEmit": false,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false
  },
  "include": [
    "./src/components/ui/**/*",
    "./src/components/assess/**/*",
    "./src/components/chat/**/*",
    "./src/components/account/**/*",
    "./src/components/activity/**/*",
    "./src/components/boss-level-grit/**/*",
    "./src/hooks/**/*",
    "./src/lib/**/*",
    "./src/pages/**/*",
    "./src/routerV2/**/*",
    "./src/router/**/*",
    "./src/contexts/**/*",
    "./src/types/**/*",
    "./src/COMPONENTS.reg.tsx",
    "./src/global.d.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}