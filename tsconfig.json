{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@types/*": ["types/*"],
      "cross-fetch": ["tests/polyfills/cross-fetch.ts"],
      "@/routes": ["src/lib/routes.ts"],
      "@/routerV2": ["src/lib/routerV2/index.ts"],
      "@/routerV2/*": ["src/lib/routerV2/*"],
      "@/guards": ["src/lib/routerV2/guards.ts"]
    },
    "jsx": "react-jsx"
  },
  "include": ["src/types.ts", "src/types/**/*", "types"],
  "exclude": ["src/**/*"]
}
